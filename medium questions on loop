"""
from math import exp,factorial
sum=s=0
x=float(input("enter the value in radian"))
n=int(input("enter upto which value of n"))              
for i in range(0,n):
    s=(x**i)/factorial(i)
    sum=sum+s
print("cal is",sum,"also inbuilt value is",exp(x)) 
