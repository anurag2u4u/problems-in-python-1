from math import factorial
n=int(input("input the value of n"))
i=0
j=0
for i in range(0,n):       
        for j in range(0,i):
            print(int(factorial(i)/(factorial(i-j)*factorial(j))),end=" ")
        i=i+1
        print('1\n')
